bindkey "^[[1;3C" forward-word
bindkey "^[[1;3D" backward-word
bindkey '^H' backward-kill-word

export PATH="/root/code/triton/build/cmake.linux-x86_64-cpython-3.12/bin:${PATH}"
export PATH="/usr/local/lib/python3.12/dist-packages/triton/:${PATH}"

export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8

